#include <Fast_LED.h>

#define NUM_LEDS 135
#define DATA_PIN 11

void setup() {
  FastLED.addLeds<WS2812B, DATA_PIN, GRB>(leds, NUM_LEDS);
  }
void loop() { 
  FadeInOut(0xff, 0x77, 0x00);
}

void FadeInOut(byte red, byte green, byte blue){
  float r, g, b;
      
  for(int i = 0; i < 256; i=i+1) { 
    r = (i/256.0)*red;
    g = (i/256.0)*green;
    b = (i/256.0)*blue;
    setAll(r,g,b);
    showStrip();
  }
     
  for(int i = 255; i >= 0; i=i-2) {
    r = (i/256.0)*red;
    g = (i/256.0)*green;
    b = (i/256.0)*blue;
    setAll(r,g,b);
    showStrip();
  }
}
